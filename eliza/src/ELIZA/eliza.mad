            LIST.(INPUT)                                                        000090
            LIST.(OUTPUT)                                                       000100
            LIST.(JUNK)                                                         000110
            LIMIT=1                                                             000120
            LSSCPY.(TREAD.(INPUT,SCRIPT),JUNK)                                  000130
            MTLIST.(INPUT)                                                      000140
